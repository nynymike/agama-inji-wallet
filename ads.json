{
    "agama-inji-wallet": {
        "projectName": "agama-inji-wallet",
        "description": "Agama Project to validate the MOSIP Inji web wallet",
        "type": "community",
        "author": "mmrraju",
        "version": "0.0.1",
        "authorWebsite": "https://github.com/mmrraju/agama-inji-wallet",
        "githubUri": "https://github.com/mmrraju/agama-inji-wallet",
        "license": "apache-2.0",
        "lastUpdated": "1763476530614",
        "tags": [
            "jans",
            "authentication",
            "authorization",
            "openid-connect"
        ],
        "noDirectLaunch": [],
        "configs": {
            "com.gluu.agama.inji.verifyCredential": {
                "agamCallbackUrl": "https://mmrraju-promoted-macaque.gluu.info/jans-auth/fl/callback",
                "injiWebBaseURL": "https://injiweb.collab.mosip.net",
                "injiVerifyBaseURL": "https://injiverify.collab.mosip.net",
                "presentationDefinition": {
                    "id": "c4822b58-7fb4-454e-b827-f8758fe27f9a",
                    "purpose": "Relying party is requesting your digital ID for the purpose of Self-Authentication",
                    "format": {
                        "ldp_vc": {
                            "proof_type": [
                                "Ed25519Signature2020"
                            ]
                        }
                    },
                    "input_descriptors": [
                        {
                            "id": "id card credential",
                            "format": {
                                "ldp_vc": {
                                    "proof_type": [
                                        "RsaSignature2018"
                                    ]
                                }
                            },
                            "constraints": {
                                "fields": [
                                    {
                                        "path": [
                                            "$.type"
                                        ],
                                        "filter": {
                                            "type": "object",
                                            "pattern": "MOSIPVerifiableCredential"
                                        }
                                    }
                                ]
                            }
                        }
                    ]
                },
                "clientMetadata": {
                    "client_name": "Agama Application",
                    "logo_uri": "https://mosip.github.io/inji-config/logos/StayProtectedInsurance.png",
                    "vp_formats": {
                        "ldp_vp": {
                            "proof_type": [
                                "Ed25519Signature2018",
                                "Ed25519Signature2020",
                                "RsaSignature2018"
                            ]
                        }
                    }
                }
            },
            "code": {}
        },
        "name": "agama-inji-wallet"
    }
}